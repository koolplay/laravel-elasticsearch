{
	"name": "cviebrock/laravel-elasticsearch",
	"description": "An easy way to use the official PHP ElasticSearch client in your Laravel applications.",
	"keywords": [
		"laravel",
		"elasticsearch",
		"search",
		"client"
	],
	"require": {
		"php": ">=5.6.6",
		"elasticsearch/elasticsearch": "~5.2",
	  	"elasticsearch/xpack" : "dev-master",
		"illuminate/support": "~4|~5",
		"monolog/monolog": "~1"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Colin Viebrock",
			"email": "colin@viebrock.ca"
		},
		{
			"name": "Brandon Martel",
			"email": "brandonmartel@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Cviebrock\\LaravelElasticsearch\\": "src/"
		}
	},
  	"repositories": [
	  {
		"type": "vcs",
		"url": "https://github.com/koolplay/elasticsearch-x-pack-php"
	  }
	],
	"minimum-stability": "stable",
	"prefer-stable": true
}
